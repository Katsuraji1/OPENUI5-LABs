<mvc:View
	controllerName="zjblessons.Worklist.controller.List"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	height="100%"
	xmlns:semantic="sap.f.semantic"
	busyIndicatorDelay="0"
>
<semantic:SemanticPage
		id="page"
		headerPinnable="false"
		toggleHeaderOnTitleClick="false"
		busy="{objectView>/busy}"
		busyIndicatorDelay="{objectView>/delay}">
		<semantic:titleHeading
			class='sapUiMarginSmall'>
			<HBox
				alignItems="Center"
			>
				<Button
				tooltip="{i18n>ttlNavBack}"
				press="onNavBack"
				icon="{i18n>iBack}"
				type="Emphasized"/>
				<Title class='sapUiSmallMarginBegin' text="{MaterialText}"/>
			</HBox>
		</semantic:titleHeading>
		<semantic:content>
			<List
				busy="{= ${listView>/busy}}"
				busyIndicatorDelay="0"
				id='list'
				growing="true"
			>
			</List>
		</semantic:content>
		</semantic:SemanticPage>
</mvc:View>